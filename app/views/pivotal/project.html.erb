<h1>Stories</h1>

<ul>
  <% @stories.each do |story| %>
    <li><%= story %></li>

  <% end %>
</ul>

<hr>

<h1>Tracker Comments</h1>

<ul>
  <% @comments.each do |comment| %>
    <li><%= comment %></li>
  <% end %>
</ul>

<hr>

<h1>Github Comments</h1>

<ul>
  <% @gh_comments.each do |comment| %>
    <% if comment == [] %>
    <% else %>
      <li><%= comment %></li>
    <% end %>
  <% end %>
</ul>
